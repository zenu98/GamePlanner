{
  "expo": {
    "name": "게임플래너",
    "slug": "gameplanner",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "gameplanner",
    "icon": "./assets/gameplanner-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/gameplanner-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#6183f2"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/gameplanner-logo.png",
        "backgroundColor": "#6183f2"
      },
      "package": "com.project.gameplanner"
    },
    "web": {
      "favicon": "./assets/gameplanner-logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "8a0def30-d202-4146-8978-25119dfbf5c2"
      }
    },
    "plugins": ["expo-font"]
  }
}
